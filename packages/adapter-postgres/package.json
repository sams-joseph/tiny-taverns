{
  "name": "@repo/adapter-postgres",
  "type": "module",
  "scripts": {
    "dev": "dotenv -e ../../.env tsx watch src/index.ts",
    "build": "dotenv -e ../../.env tsc -b",
    "migrate": "dotenv -e ../../.env tsx src/scripts/migrator.ts",
    "reset": "dotenv -e ../../.env tsx src/scripts/reset.ts",
    "seed": "dotenv -e ../../.env tsx src/scripts/seed.ts"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts",
    "./*": "./src/*.ts",
    "./migrate": "./src/scripts/migrator.ts",
    "./reset": "./src/scripts/reset.ts",
    "./seed": "./src/scripts/seed.ts",
    "./internal/*": null
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "typescript": "catalog:"
  },
  "dependencies": {
    "@effect/platform": "catalog:",
    "@effect/platform-node": "catalog:",
    "@effect/sql": "catalog:",
    "@effect/sql-pg": "catalog:",
    "effect": "catalog:"
  }
}
