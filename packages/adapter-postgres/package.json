{
  "name": "@repo/adapter-postgres",
  "type": "module",
  "scripts": {
    "dev": "dotenv -e ../../.env tsx watch src/index.ts",
    "build": "dotenv -e ../../.env tsx",
    "migrate": "dotenv -e ../../.env tsx src/scripts/migrator.ts",
    "reset": "dotenv -e ../../.env tsx src/scripts/reset.ts"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts",
    "./*": "./src/*.ts",
    "./migrate": "./src/scripts/migrator.ts",
    "./reset": "./src/scripts/reset.ts",
    "./internal/*": null
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "typescript": "latest"
  },
  "dependencies": {
    "@effect/platform": "^0.94.3",
    "@effect/platform-node": "^0.104.1",
    "@effect/sql": "^0.49.0",
    "@effect/sql-pg": "^0.50.2",
    "effect": "^3.19.16"
  }
}
